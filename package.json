{
  "name": "KyanJukebox",
  "version": "0.0.1",
  "description": "Kyan Jukebox",
  "repository": "",
  "main": "js/app.js",
  "dependencies": {
    "babel-plugin-transform-class-properties": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.3.0",
    "classnames": "^2.1.3",
    "flux": "^2.0.1",
    "keymirror": "~0.1.0",
    "object-assign": "^1.0.0",
    "react": "^15.0.2",
    "react-dom": "^15.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "browserify": "^6.2.0",
    "envify": "^3.0.0",
    "jasmine-node": "^1.14.5",
    "reactify": "^0.15.2",
    "uglify-js": "~2.4.15",
    "watchify": "^3.7.0"
  },
  "scripts": {
    "start": "watchify -t babelify -v -d js/app.js -o js/bundle.js",
    "build": "browserify . -t [envify --NODE_ENV production] -t [babelify] | uglifyjs -cm > js/bundle.min.js",
    "test": "jasmine-node specs"
  },
  "babel": {
    "plugins": [
      "transform-class-properties"
    ],
    "presets": [
      "es2015",
      "react"
    ]
  },
  "author": "Kyan",
  "browserify": {
    "transform": [
      "reactify",
      "envify"
    ]
  },
  "jest": {
    "rootDir": "./js"
  }
}
